<assembly xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://maven.apache.org/ASSEMBLY/2.1.0"
          xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.1.0 http://maven.apache.org/xsd/assembly-2.1.0.xsd">
    <id>distribution</id>
    <formats>
        <format>zip</format>
    </formats>
    <fileSets>
        <fileSet>
            <directory>${project.basedir}/target/native-windows/jre</directory>
            <outputDirectory>jre</outputDirectory>
            <includes>
                <include>**/*</include>
            </includes>
        </fileSet>
        <fileSet>
            <directory>${project.basedir}/target/native-windows</directory>
            <outputDirectory></outputDirectory>
            <includes>
                <include>george.jar</include>
            </includes>
        </fileSet>
        <fileSet>
            <directory>${project.basedir}/assemble</directory>
            <outputDirectory></outputDirectory>
            <includes>
                <include>george.bat</include>
            </includes>
        </fileSet>
    </fileSets>
</assembly>